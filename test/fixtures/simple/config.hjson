{
  "locals": {
    "name": "Caleb"
  }
  "files": {
    "**/*.svg": "svgo"
    "**/*.js": "uglify-js"
    "**/*.coffee": "coffee-script | uglify-js"
    "**/*.styl": {
      "transforms": "stylus | autoprefixer | csso"
      "depends_on": [
        "**/*.css"
        "**/*.styl"
      ]
    }
    "**/*.pug": [
      "pug"
      "html-minifier"
    ]
  }
}
