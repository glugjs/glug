#!/usr/bin/env node

var program = require('commander')

program

  .version('0.0.5')

  .command('watch [directory]', 'start a server')

  .command('watch2 [directory]', 'start a server')

  .command('watch3 [directory]', 'start a server')

  .command('compile [directory]', 'build the project')

  .parse(process.argv)
